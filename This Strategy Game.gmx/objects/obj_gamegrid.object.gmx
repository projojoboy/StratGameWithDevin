}

//populate neighbor lists!
for(xx = 0; xx &lt; 25; xx += 1;){
    for(yy = 0; yy &lt; 25; yy += 1;){
        
        node = map[xx, yy];
        
        //add left neighbor
        if(xx &gt; 0) {
            ds_list_add(node.neighbors, map[xx - 1, yy]);
        }
        
        //add right neighbor
        if(xx &lt; 25 - 1){
            ds_list_add(node.neighbors, map[xx + 1, yy]);
        }
        
        //add top neighbor
        if(yy &gt; 0){
            ds_list_add(node.neighbors, map[xx, yy - 1]);
        }
        
        //add bottom neighbor
        if(yy &lt; 25 - 1){
            ds_list_add(node.neighbors, map[xx, yy + 1]);
        }
    }    
}
